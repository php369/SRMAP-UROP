{
  "name": "@srm-portal/api",
  "version": "1.0.0",
  "description": "SRM Project Portal API Server",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prebuild": "node prebuild.js",
    "build": "tsc --project tsconfig.build.json",
    "build:prod": "tsc --project tsconfig.deploy.json",
    "build:deploy": "node build-deploy.js",
    "build:deploy:ts": "tsc --project tsconfig.deploy.json --noEmit false || true",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "seed": "tsx src/scripts/seed.ts",
    "clear-data": "tsx src/scripts/clear-data.ts",
    "clear-projects": "tsx src/scripts/clearProjectsAndApplications.ts",
    "check-data": "tsx src/scripts/check-data.ts",
    "cleanup-schema": "node --import tsx src/scripts/cleanup-schema.ts",
    "cleanup-models": "tsx src/scripts/cleanup-removed-models.ts",
    "reset-initialize": "tsx src/scripts/reset-and-initialize-db.ts",
    "verify-db": "tsx src/scripts/verify-db-state.ts",
    "final-cleanup": "tsx src/scripts/final-db-cleanup.ts",
    "reset-db": "tsx src/scripts/reset-database.ts",
    "migrate-meeting-status": "tsx src/scripts/migrate-meeting-status.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^9.4.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "googleapis": "^128.0.0",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "fast-check": "^4.3.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.3.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "srm",
    "university",
    "project",
    "portal",
    "api",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "SRM University-AP",
  "license": "MIT"
}