# SRM Project Portal - Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_API_VERSION=v1

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Application Configuration
VITE_APP_NAME=SRM Project Portal
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_ANIMATIONS=true

# File Upload Configuration
VITE_MAX_FILE_SIZE=50MB
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,txt,jpg,jpeg,png,zip,rar

# Real-time Features
VITE_ENABLE_WEBSOCKETS=true
VITE_WEBSOCKET_URL=ws://localhost:3001

# External Services
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
VITE_SENTRY_DSN=your-sentry-dsn-for-error-tracking

# Development Tools
VITE_ENABLE_REACT_QUERY_DEVTOOLS=true
VITE_ENABLE_REDUX_DEVTOOLS=true